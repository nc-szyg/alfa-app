{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "amigoscode-react",
      "image": "szymciogrosik/amigoscode-react:16.01.2023.23.39.48",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5173
        }
      ]
    },
    {
      "name": "amigoscode-api",
      "image": "szymciogrosik/amigoscode-api:20.04.2023.13.39.03",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "test"
        }
      ]
    }
  ]
}